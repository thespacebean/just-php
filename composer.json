{
  "name": "vanilla/vanilla",
  "description": "Vanilla PHP with Twig Template Engine",
  "require": {
    "ext-pdo": "*",
    "twig/twig": "^3.0"
  },
    "require-dev": {
      "phpunit/phpunit": "^11.5.3"
    },
  "autoload": {
    "psr-4": {
      "App\\": [
        "src/app/",
        "app/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --do-not-cache-result"
  }
 }
